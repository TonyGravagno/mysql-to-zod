{
  "name": "mysql-to-zod",
  "version": "1.0.8b",
  "description": "Convert MySQL schemas into Zod schemas",
  "source": "src/main.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "bin": {
    "mysql-to-zod": "dist/main.js"
  },
  "scripts": {
    "mysql-to-zod": "tsx ./src/main.ts",
    "dev": "tsx ./src/main.ts",
    "start": "cross-env NODE_ENV=production node ./dist/main.js",
    "build": "zx ./build.mjs && zx ./addShebang.mjs",
    "lint-fix": "eslint --cache --fix src/ && prettier --write src/",
    "test": "jest",
    "type": "tsc --noEmit"
  },
  "keywords": [
    "typescript",
    "zod",
    "mysql",
    "schema",
    "sql"
  ],
  "author": "yuumillar",
  "license": "MIT",
  "devDependencies": {
    "@swc/core": "^1.3.44",
    "@swc/jest": "^0.2.24",
    "@types/eslint": "^8.37.0",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/prettier": "^2.7.2",
    "@types/ramda": "^0.28.23",
    "@types/rimraf": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "cross-env": "^7.0.3",
    "crossenv": "^0.0.2-security",
    "esbuild": "^0.17.14",
    "eslint": "^8.37.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-base-typescript": "^1.1.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.5.0",
    "rimraf": "^4.4.1",
    "tsc": "^2.0.4",
    "tsx": "^3.12.6",
    "typescript": "^5.0.3"
  },
  "dependencies": {
    "camelcase": "^7.0.1",
    "commander": "^10.0.0",
    "cosmiconfig": "^8.1.3",
    "cosmiconfig-typescript-loader": "^4.3.0",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "fp-ts": "^2.13.1",
    "fs-extra": "^11.1.1",
    "mysql2": "^3.2.0",
    "node-sql-parser": "^4.6.6",
    "prettier": "^2.8.7",
    "ramda": "^0.28.0",
    "ts-case-convert": "^2.0.2",
    "ts-node": "^10.9.1",
    "zod": "^3.21.4"
  },
  "volta": {
    "node": "18.14.1"
  }
}