{
  "name": "mysql-to-zod",
  "version": "2.0.4",
  "description": "Convert MySQL schemas into Zod schemas",
  "source": "src/main.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "bin": {
    "mysql-to-zod": "dist/main.js"
  },
  "scripts": {
    "mysql-to-zod": "tsx ./src/main.ts",
    "dev": "tsx ./src/main.ts",
    "build": "zx ./build.mjs && zx ./addShebang.mjs",
    "test": "vitest",
    "type": "tsc --noEmit",
    "lint": "pnpm biome check --apply ./src",
    "fmt": "pnpm biome format --write ./src",
    "pub": "zx ./build.mjs && zx ./addShebang.mjs && npm publish",
    "knip": "knip"
  },
  "keywords": [
    "typescript",
    "zod",
    "mysql",
    "schema",
    "sql"
  ],
  "author": "yuumillar",
  "license": "MIT",
  "devDependencies": {
    "@swc/core": "^1.3.70",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.4.2",
    "@types/ramda": "^0.29.3",
    "@types/rimraf": "^3.0.2",
    "cross-env": "^7.0.3",
    "esbuild": "^0.18.15",
    "rimraf": "^5.0.1",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "vitest": "^0.33.0",
    "zod": "^3.22.4",
    "zx": "^7.2.3"
  },
  "dependencies": {
    "@biomejs/biome": "^1.4.1",
    "@prettier/sync": "^0.3.0",
    "camelcase": "^7.0.1",
    "commander": "^11.0.0",
    "cosmiconfig": "^8.2.0",
    "cosmiconfig-typescript-loader": "^4.3.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "fp-ts": "^2.16.0",
    "fs-extra": "^11.1.1",
    "immer": "^10.0.2",
    "mysql2": "^3.5.2",
    "node-sql-parser": "^4.9.0",
    "prettier": "^3.0.0",
    "ramda": "^0.29.0",
    "ts-case-convert": "^2.0.2",
    "ts-morph": "^20.0.0",
    "ts-node": "^10.9.1",
    "ts-pattern": "^5.0.4"
  },
  "volta": {
    "node": "18.17.0",
    "npm": "9.8.1"
  }
}